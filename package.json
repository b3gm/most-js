{
  "name": "@b3gm/most",
  "version": "1.2.3",
  "description": "Dependency Injection for Javascript",
  "main": "dist/most.js",
  "typings": "dist/most.d.ts",
  "scripts": {
    "test": "jest --coverage",
    "build": "tsc",
    "build-test": "tsc && npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.lucky-trade.de/frontend/most-js.git"
  },
  "keywords": [
    "dependency_injection",
    "js",
    "ts"
  ],
  "author": "b3gm",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^23.3.0",
    "jest": "^23.4.1",
    "mocha-sonar-reporter": "^0.1.6",
    "ts-jest": "^23.0.1",
    "typescript": "^2.9.2"
  },
  "publishConfig": {
    "registry": "https://nexus.lucky-trade.de/repository/npm-private/"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
